<h1>Employee Registration </h1>
<p>Create Employee</p>

<%= simple_form_for @user do |f| %>
  <%= f.input :name, label: "Employee Name" %>
  <%= f.input :address1, label: "Employee Address" %>
  <%= f.input :city, collection: @cities, value_method: :name, :include_blank => false %>
  <%= f.input :email, label: "Email" %>
  <%= f.input :role, collection: @roles, value_method: :name,:include_blank => false %>
  <%= f.input :telephoneNo, label: "Telephone Number" %>
  <%= f.button :submit %>
<% end %>


